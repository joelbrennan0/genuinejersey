<table>
  
  <thead>
    <tr>
      <th>Email</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
      
    <% @users.each do |user| %>

      <tr>
        <td><%= user.email %></td>
        <td>
          <%= link_to 'View', user %>
          <%= link_to 'Edit', edit_user_path(user) %>
          <%= link_to 'Delete', user_path(user), method: :delete, data: { confirm: "Are you sure you want to delete this user?"} %>
        </td>
      </tr>

    <% end %>

  </tbody>
</table>